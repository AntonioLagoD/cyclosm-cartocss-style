- id: roads_high
  Datasource:
    dbname: osm
    extent: "-20037508.34 -20037508.34 20037508.34 20037508.34"
    geometry_field: way
    id: roads_high
    key_field: ''
    project: osm-bright-master
    srs: 
    table: |-
      ( 
        SELECT way, COALESCE(highway, railway) AS type, 0 AS tunnel, 0 AS bridge, access, 'fill' AS render,
        CASE
          WHEN highway IN ('motorway', 'trunk') THEN 'motorway'
          WHEN highway IN ('primary', 'secondary') THEN 'mainroad'
          WHEN highway IN ('motorway_link', 'trunk_link', 'primary_link', 'secondary_link', 'tertiary', 'tertiary_link', 'residential', 'unclassified', 'road', 'living_street') THEN 'minorroad'
          WHEN highway IN ('service', 'track') THEN 'service'
          WHEN highway='cycleway' THEN 'cycle'
          WHEN highway IN ('path', 'footway', 'pedestrian', 'steps', 'bridleway') THEN CASE WHEN bicycle IN ('yes', 'designated') THEN 'cycle' ELSE 'pedestrian' END
          WHEN railway IN ('light_rail', 'subway', 'narrow_gauge', 'rail', 'tram') THEN 'railway'
        ELSE 'other' END AS stylegroup,
        tags->'cycleway' AS cycleway,
        tags->'cycleway:left' AS cycleway_left,
        tags->'cycleway:right' AS cycleway_right,
        CASE
          WHEN highway='cycleway' THEN 10*layer::integer+9
          WHEN highway IN ('path', 'footway', 'pedestrian', 'steps', 'bridleway') AND bicycle IN ('yes', 'designated') THEN 10*layer::integer+9
        ELSE z_order END AS z_order
        FROM planet_osm_line
        WHERE (highway IS NOT NULL OR railway IS NOT NULL)
          AND (tunnel IS NULL OR tunnel = 'no')
          AND (bridge IS NULL OR bridge = 'no')
        ORDER BY z_order
      ) AS data
    type: postgis
  class: ''
  geometry: linestring
  name: roads_high
  srs: "+proj=merc +a=6378137 +b=6378137 +lat_ts=0.0 +lon_0=0.0 +x_0=0.0 +y_0=0.0 +k=1.0 +units=m +nadgrids=@null +wktext +no_defs +over"
  srs-name: '900913'
